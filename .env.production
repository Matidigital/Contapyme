# Production Environment Variables for Netlify
# These will be configured in Netlify dashboard

# Supabase (to be set in Netlify)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# App
NEXT_PUBLIC_APP_URL=https://your-netlify-domain.netlify.app

# Environment
NODE_ENV=production